{  
    "dockerComposeFile": ["./docker-compose.yml"],
    "extensions": [
		"haskell.haskell",
		"justusadam.language-haskell",
		"isudox.vscode-jetbrains-keybindings",
		"mhutchie.git-graph"
	],
    "features": {
        "haskell.manageHLS": "GHCup"
    },
    "forwardPorts": [4205],
    "service": "haskell",
    "shutdownAction": "stopCompose",
    "workspaceFolder": "/opt/app"
  }